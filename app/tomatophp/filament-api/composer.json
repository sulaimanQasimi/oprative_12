{
    "name": "tomatophp/filament-api",
    "type": "library",
    "description": "Generate APIs from your filament resource using single line of code",
    "keywords": [
        "php",
        "laravel",
        "filamentphp",
        "generator",
        "tomatophp",
        "filament-plugin",
        "api",
        "rest"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentApi\\FilamentApiServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "filament/filament": "^3.0.0",
        "calebporzio/sushi": "*"
    }
}
